<p id="notice"><%= notice %></p>

<h1>Jovems</h1>

<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th>Data nascimento</th>
      <th>Ano inicio</th>
      <th>Endereco</th>
      <th>Telefone</th>
      <th>Celular</th>
      <th>Email</th>
      <th>Observacao</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @jovems.each do |jovem| %>
      <tr>
        <td><%= jovem.nome %></td>
        <td><%= jovem.data_nascimento %></td>
        <td><%= jovem.ano_inicio %></td>
        <td><%= jovem.endereco %></td>
        <td><%= jovem.telefone %></td>
        <td><%= jovem.celular %></td>
        <td><%= jovem.email %></td>
        <td><%= jovem.observacao %></td>
        <td><%= link_to 'Show', jovem %></td>
        <td><%= link_to 'Edit', edit_jovem_path(jovem) %></td>
        <td><%= link_to 'Destroy', jovem, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Jovem', new_jovem_path %>
